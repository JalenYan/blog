SECRETS=$(kamal secrets fetch --adapter 1password --account my --from DeploySecrets/blog KAMAL_REGISTRY_PASSWORD RAILS_MASTER_KEY POSTGRES_PASSWORD MADMIN_USER MADMIN_PASSWORD)
KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract KAMAL_REGISTRY_PASSWORD ${SECRETS})
RAILS_MASTER_KEY=$(kamal secrets extract RAILS_MASTER_KEY ${SECRETS})
POSTGRES_PASSWORD=$(kamal secrets extract POSTGRES_PASSWORD ${SECRETS})
MADMIN_USER=$(kamal secrets extract MADMIN_USER ${SECRETS})
MADMIN_PASSWORD=$(kamal secrets extract MADMIN_PASSWORD ${SECRETS})
